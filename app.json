{
    "id": "/dashboard-app",

    "mem": 256.0,
    "instances": 2,

    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "mesoscloud/dashboard-app",

            "network": "BRIDGE",

            "portMappings": [
                { "containerPort": 4000, "servicePort": 80 }
            ]
        }
    },

    "healthChecks": [
        { "protocol": "HTTP", "portIndex": 0 }
    ]
}
